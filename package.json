{
    "name": "symfony-code-snippets",
    "displayName": "Symfony code snippets",
    "icon": "icon.png",
    "description": "Over 400 Symfony code snippets. Just type the letters 'sf:' or 'sf5' ( for symfony 5.x ) to get a list of all available Symfony code snippets.",
    "publisher": "nadim-vscode",
    "author": {
        "name": "Nadim Al Abdou",
        "url": "https://nalabdou.com"
    },
    "homepage": "https://github.com/nalabdou/Symfony-code-snippets/blob/master/README.md",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/nalabdou/Symfony-code-snippets"
    },
    "bugs": "https://github.com/nalabdou/Symfony-code-snippets/issues",
    "version": "2.1.1",
    "engines": {
        "vscode": "^1.73.1"
    },
    "categories": [
        "Snippets",
        "Language Packs",
        "Programming Languages",
        "Extension Packs",
        "Other"
    ],
    "keywords": [
        "snippets",
        "completion",
        "symfony",
        "php"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./symfony/6.x/components/command.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/httpfoundation.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/controller.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/entity.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/validator.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/packages/framwork.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/3rdparty/doctrine.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/contributes/nalabdou.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/5.x/symfony.json"
            }
        ],
        "configuration": {
            "title": "Symfony code snippets",
            "properties": {
                "symfony.code.snippets.hideWelcomeMessage": {
                    "type": "boolean",
                    "default": false,
                    "description": "Hide the wellcome message."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@types/mocha": "^10.0.0",
        "graceful-fs": "^4.2.10",
        "php-parser": "^3.1.2",
        "strip-json-comments": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^18.11.19",
        "@types/vscode": "^1.73.1",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.44.0",
        "eslint": "^8.34.0",
        "typescript": "^4.9.3"
    },
    "qna": "marketplace",
    "extensionPack": [
        "nadim-vscode.twig-code-snippets",
        "nadim-vscode.infinity-dark-theme"
    ]
}