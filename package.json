{
    "name": "symfony-code-snippets",
    "displayName": "Symfony code snippets",
    "icon": "icon.png",
    "description": "Over 100 Symfony code snippets. Just type the letters 'sf' to get a list of all available Symfony code snippets.",
    "publisher": "nadim-vscode",
    "author": {
        "name": "Nadim Al Abdou",
        "url": "https://nalabdou.com"
    },
    "homepage": "https://github.com/nalabdou/Symfony-code-snippets/blob/master/README.md",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/nalabdou/Symfony-code-snippets"
    },
    "bugs": "https://github.com/nalabdou/Symfony-code-snippets/issues",
    "version": "1.2.8",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Snippets",
        "Language Packs",
        "Programming Languages",
        "Extension Packs",
        "Other"
    ],
    "keywords": [
        "snippets",
        "completion",
        "symfony",
        "php"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./symfony/6.x/components/controller.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/command.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/twig-component.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/entity.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/components/validator.code-snippets"
            },
            {
                "language": "php",
                "path": "./symfony/6.x/packages/framwork.code-snippets"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^16.18.3",
        "@types/vscode": "^1.73.1",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "eslint": "^8.28.0",
        "typescript": "^4.9.3"
    },
    "galleryBanner": {
        "color": "#353091",
        "theme": "dark"
    },
    "qna": "marketplace"
}